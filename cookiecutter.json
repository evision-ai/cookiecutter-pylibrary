{
    "full_name": "eVision Awesome Project",
    "email": "dev@evision.ai",
    "website": "https://evision.ai",
    "package_namespace": "evision",
    "package_name": "lib",
    "project_name": "{{ cookiecutter.package_namespace}}-{{ cookiecutter.package_name}}",
    "repo_name": "{{ cookiecutter.project_name|lower|replace(' ','-') }}",
    "repo_hosting": ["github.com", "gitlab.com", "192.100.1.11:3123","other domain not listed"],
    "repo_hosting_domain": "{{ cookiecutter.repo_hosting if cookiecutter.repo_hosting != 'other domain not listed' else '' }}",
    "repo_username": "evision-ai",
    "distribution_name": "{{ cookiecutter.package_namespace|replace('_','-') }}-{{ cookiecutter.package_name|replace('_','-') }}",
    "project_short_description": "An example package. Generated with cookiecutter-pylibrary.",
    "release_date": "today",
    "year_from": [
        "2018",
        "2019"
    ],
    "year_to": "{% now 'utc+8', '%Y' %}",
    "version": "0.1.0",
    "license": ["BSD 2-Clause License", "BSD 3-Clause License", "MIT license", "ISC license", "Apache Software License 2.0", "no"],
    "c_extension_support": ["no", "yes", "cffi", "cython"],
    "c_extension_optional": ["no", "yes"],
    "c_extension_module": "_{{ cookiecutter.package_namespace}}_{{ cookiecutter.package_name}}",
    "c_extension_function": "longest",
    "c_extension_test_pypi": ["no", "yes"],
    "c_extension_test_pypi_username": "{{ cookiecutter.repo_username }}",
    "test_matrix_configurator": ["no", "yes"],
    "test_matrix_separate_coverage": ["no", "yes"],
    "test_runner": ["pytest", "nose"],
    "setup_py_uses_test_runner": ["no", "yes"],
    "setup_py_uses_setuptools_scm": ["no", "yes"],
    "pypi_badge": ["no", "yes"],
    "pypi_disable_upload": ["yes", "no"],
    "allow_tests_inside_package": ["yes", "no"],
    "linter": ["flake8", "pylama"],
    "command_line_interface": ["argparse", "plain", "click", "no"],
    "command_line_interface_bin_name": "{{ cookiecutter.distribution_name }}",
    "coveralls": ["no", "yes"],
    "coveralls_token": "[Required for Appveyor, take it from https://coveralls.io/github/{{ cookiecutter.repo_username }}/{{ cookiecutter.repo_name }}]",
    "codecov": ["no", "yes"],
    "landscape": ["no", "yes"],
    "scrutinizer": ["no", "yes"],
    "codacy" : ["no", "yes"],
    "codacy_projectid" : "[Get ID from https://app.codacy.com/app/{{ cookiecutter.repo_username }}/{{ cookiecutter.repo_name }}/settings]",
    "codeclimate" : ["no", "yes"],
    "sphinx_docs": ["yes", "no"],
    "sphinx_theme": ["sphinx-py3doc-enhanced-theme", "sphinx-rtd-theme"],
    "sphinx_doctest": ["yes", "no"],
    "sphinx_docs_hosting": "https://{{ cookiecutter.repo_name|replace('.', '') }}.readthedocs.io/",
    "travis": ["no", "yes"],
    "travis_osx": ["no", "yes"],
    "appveyor": ["no", "yes"],

    "_extensions": ["jinja2_time.TimeExtension"]
}
